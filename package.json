{
  "name": "@photosynthesic/nubui",
  "version": "0.1.4",
  "description": "Custom SVG icons instantly usable with Tailwind CSS. Framework-agnostic with mask/inline/img modes.",
  "main": "./lib/index.cjs",
  "module": "./lib/index.mjs",
  "types": "./types/index.d.ts",
  "bin": {
    "nubui": "./bin/nubui.js"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./icon": {
      "types": "./types/icon/index.d.ts",
      "import": "./lib/icon/index.mjs",
      "require": "./lib/icon/index.cjs"
    }
  },
  "scripts": {
    "build": "npm run clean && tsup",
    "icon:masks": "node bin/nubui.js icon:masks",
    "icon:preview": "node bin/nubui.js icon:preview",
    "icon:build": "node bin/nubui.js icon:build",
    "icon:clean": "node bin/nubui.js icon:clean",
    "prepublishOnly": "npm run build",
    "test": "vitest run && npm run test:package",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ci": "vitest run --coverage --reporter=json --reporter=verbose",
    "test:package": "sh ./test-package.sh",
    "clean": "rm -rf lib .nubui-cache.json"
  },
  "keywords": [
    "tailwindcss",
    "icons",
    "svg",
    "components",
    "scss",
    "typescript"
  ],
  "dependencies": {
    "svgo": "^3.3.2"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^27.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "lib/**/*",
    "types/**/*",
    "bin/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/photosynthesic/nubui.git"
  },
  "author": "Photosynthesic",
  "license": "MIT"
}
